{
  "name": "starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "commit": "git-cz",
    "lint": "eslint --fix .",
    "format": "prettier --write '**/*.{ts,tsx,js,vue,css,scss,html,json}'",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "api": "tsg-tgjz -c ./src/api/tsg.config.ts"
  },
  "dependencies": {
    "@ant-design-vue/use": "^0.0.1-alpha.9",
    "ant-design-vue": "^2.1.2",
    "axios": "^0.21.1",
    "path-to-regexp": "^6.2.0",
    "vue": "^3.0.11",
    "vue-router": "^4.0.6",
    "vxe-table": "^4.0.9",
    "xe-utils": "^3.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@types/prettier": "^2.2.3",
    "@types/swagger-schema-official": "^2.0.21",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-vue-jsx": "^1.1.3",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "commitizen": "^4.2.3",
    "commitlint-config-cz": "^0.13.2",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.23.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "4",
    "less": "^4.1.1",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "tsg-tgjz": "^3.2.0",
    "typescript": "^4.2.4",
    "vite": "^2.1.5",
    "vite-plugin-mockit": "^1.0.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,vue,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
