{
  "name": "starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "uat": "vite build --mode uat",
    "release": "vite build",
    "preview": "vite preview --base /",
    "type:check": "tsc --noEmit --skipLibCheck",
    "commit": "cz-customizable",
    "lint": "eslint --fix .",
    "format": "prettier --write '**/*.{ts,tsx,js,vue,css,scss,html,json}'",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "api": "tsg -c ./src/api/tsg.config.ts",
    "gen:icon-types": "node scripts/icon-types.mjs"
  },
  "dependencies": {
    "@abraham/reflection": "^0.8.0",
    "ant-design-vue": "^3.0.0-beta.5",
    "axios": "0.21.1",
    "injection-js": "^2.4.0",
    "path-to-regexp": "^6.2.0",
    "ts-deepmerge": "^1.1.0",
    "ts-essentials": "^9.1.2",
    "tslib": "^2.3.1",
    "vue": "^3.2.23",
    "vue-router": "^4.0.12",
    "vue3-oop": "^0.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@nexhome/yorkie": "^2.0.8",
    "@types/node": "^17.0.5",
    "@types/prettier": "^2.4.3",
    "@types/swagger-schema-official": "^2.0.22",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue3-oop/plugin-vue-jsx": "^1.4.0",
    "co": "4",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "less": "^4.1.2",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "sass": "^1.47.0",
    "ts-gear": "^4.3.0",
    "typescript": "^4.5.4",
    "vite": "2.7.10",
    "vite-plugin-html": "^2.1.2",
    "vite-plugin-imp": "^2.1.3",
    "vite-plugin-mockit": "^1.0.2",
    "vite-plugin-svg-icons": "^1.1.0",
    "webpack-aliyun-oss": "^0.5.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{ts,tsx,vue,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
