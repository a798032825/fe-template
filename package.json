{
  "name": "starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vuedx-typecheck . && vite build",
    "commit": "git-cz  "
  },
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@vitejs/plugin-vue": "^1.0.4",
    "@vitejs/plugin-vue-jsx": "^1.0.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.4.1",
    "@vuedx/typescript-plugin-vue": "^0.4.1",
    "commitizen": "^4.2.3",
    "commitlint-config-cz": "^0.13.2",
    "cz-customizable": "^6.3.0",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.12"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 100,
    "arrowParens": "always",
    "trailingComma": "all"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "commitlint": {
    "extends": [
      "cz"
    ],
    "rules": {
      "type-empty": [2, "never"],
      "type-subject": [2, "never"]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
