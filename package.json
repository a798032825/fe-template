{
  "name": "starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "uat": "vite build --mode uat",
    "release": "vite build",
    "preview": "vite preview --base /",
    "type:check": "tsc --noEmit --skipLibCheck",
    "commit": "cz-customizable",
    "lint": "eslint --fix .",
    "format": "prettier --write '**/*.{ts,tsx,js,vue,css,scss,html,json}'",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "api": "tsg -c ./src/api/tsg.config.ts",
    "gen:icon-types": "node scripts/icon-types.mjs"
  },
  "dependencies": {
    "@abraham/reflection": "^0.9.1",
    "axios": "0.21.1",
    "injection-js": "^2.4.0",
    "path-to-regexp": "^6.2.0",
    "ts-deepmerge": "^2.0.1",
    "ts-essentials": "^9.1.2",
    "tslib": "^2.3.1",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vue3-oop": "^0.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@nexhome/yorkie": "^2.0.8",
    "@types/node": "^17.0.21",
    "@types/prettier": "^2.4.4",
    "@types/swagger-schema-official": "^2.0.22",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue3-oop/plugin-vue-jsx": "^1.4.0",
    "autoprefixer": "^10.4.2",
    "co": "4",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "less": "^4.1.2",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "tailwindcss": "^3.0.23",
    "ts-gear": "^4.6.1",
    "typescript": "^4.6.2",
    "vite": "2.8.6",
    "vite-plugin-html": "^3.1.0",
    "vite-plugin-imp": "^2.1.6",
    "vite-plugin-mockit": "^1.0.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "webpack-aliyun-oss": "^0.5.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
